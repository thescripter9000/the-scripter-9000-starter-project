local HWIDTable = loadstring(game:HttpGet("https://pastebin.com/raw/KGgrmnvd"))();
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()

for i,v in pairs(HWIDTable) do
    if v == HWID then
            print("Hardware ID Passed, script is now loading...")
            loadstring(game:HttpGet('https://raw.githubusercontent.com/Spencer59/scripyv2/main/silent%20aim%20for%20normal%20game'))();
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Spencer59/scripyv2/main/message%20thing'))()
            if not HWID then
            loadstring(game:HttpGet("https://pastebin.com/raw/NiNwYYVd"))();
        end
    end
end